{
  "name": "@openzeppelin/transaction-form-builder-templates",
  "version": "1.0.0",
  "description": "Export templates for Transaction Form Builder",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "format": "prettier --write \"./**/*.{ts,tsx,css,json,md}\" --config ../../.prettierrc",
    "format:check": "prettier --check \"./**/*.{ts,tsx,css,json,md}\" --config ../../.prettierrc",
    "fix-all": "pnpm format && pnpm lint:fix"
  },
  "keywords": [
    "templates",
    "export",
    "blockchain",
    "react"
  ],
  "author": "OpenZeppelin",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.8.2",
    "prettier": "^3.5.3"
  }
}
